{
  parser: "babel-eslint",
  extends: "airbnb",
  plugins: [
    "react",
    "jsx-a11y",
    "import"
  ],
  env: {
    browser: true,
    node: true,
    jest: true
  },
  rules: {
    "import/prefer-default-export": 0,
    "jsx-a11y/label-has-for": 0,
    "react/forbid-prop-types": 0,
    "no-continue": 0,
    "class-methods-use-this": 0
  }
}
